<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qiang.manicurists">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.CAMERA" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:launchMode="singleTask"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".activity.LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.NewOrHotActivity" />
        <activity android:name=".activity.GoodsListActivity" />
        <activity android:name=".activity.GoodsActivity" />
        <activity android:name=".activity.BuyActivity" />
        <activity android:name=".activity.RatedActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity
            android:name=".activity.PhotoActivity"
            android:theme="@style/Transparent" />
        <activity android:name=".activity.RatedEditActivity"></activity>
    </application>

</manifest>