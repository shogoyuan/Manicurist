<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/photo_root_rel_id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.qiang.manicurists.activity.PhotoActivity">

    <com.qiang.manicurists.view.MyViewPager
        android:id="@+id/photo_viewpager_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <LinearLayout
        android:id="@+id/photo_viewpager_cyclelinear_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"/>


</RelativeLayout>
